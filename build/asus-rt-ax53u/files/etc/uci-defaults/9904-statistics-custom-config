uci -q batch << EOI
set luci_statistics.collectd_sensors.enable='1'
set luci_statistics.collectd_sensors.IgnoreSelected='1'
del luci_statistics.collectd_irq.enable
del luci_statistics.collectd_dns.enable
del luci_statistics.collectd_ping.enable
set luci_statistics.collectd.Include='/etc/collectd/conf.d/*.conf'
set luci_statistics.collectd_ipstatistics.enable='1'
commit luci_statistics
EOI
