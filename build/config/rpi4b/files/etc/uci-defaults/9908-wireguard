uci batch << EOI
set network.{{ .Env.WIREGUARD_INTERFACE_TEMPLATE }}=interface
set network.{{ .Env.WIREGUARD_INTERFACE_TEMPLATE }}.proto='wireguard'
set network.{{ .Env.WIREGUARD_INTERFACE_TEMPLATE }}.force_link='1'
set network.{{ .Env.WIREGUARD_INTERFACE_TEMPLATE }}.private_key='{{ .Env.VPN_PRIVATE_KEY_TEMPLATE }}'
set network.{{ .Env.WIREGUARD_INTERFACE_TEMPLATE }}.listen_port='{{ .Env.VPN_LISTEN_PORT_TEMPLATE }}'
set network.{{ .Env.WIREGUARD_INTERFACE_TEMPLATE }}.addresses='{{ .Env.VPN_ADDRESSES_TEMPLATE }}'
commit network
EOI

{{ .Env.VPN_CUSTOM_CLIENTS_CONFIG_TEMPLATE }}

uci commit network

/etc/init.d/network restart
