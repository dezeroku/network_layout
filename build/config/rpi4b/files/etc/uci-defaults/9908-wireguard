uci batch << EOI
set network.{{ .WIREGUARD_INTERFACE_TEMPLATE }}=interface
set network.{{ .WIREGUARD_INTERFACE_TEMPLATE }}.proto='wireguard'
set network.{{ .WIREGUARD_INTERFACE_TEMPLATE }}.force_link='1'
set network.{{ .WIREGUARD_INTERFACE_TEMPLATE }}.private_key='{{ .VPN_PRIVATE_KEY_TEMPLATE }}'
set network.{{ .WIREGUARD_INTERFACE_TEMPLATE }}.listen_port='{{ .VPN_LISTEN_PORT_TEMPLATE }}'
set network.{{ .WIREGUARD_INTERFACE_TEMPLATE }}.addresses='{{ .VPN_ADDRESSES_TEMPLATE }}'
commit network
EOI

{{ .VPN_CUSTOM_CLIENTS_CONFIG_TEMPLATE }}

uci commit network

/etc/init.d/network restart
